{
  "name": "lorawan-airtime-ui",
  "description": "LoRaWAN airtime calculator",
  "version": "1.0.0",
  "author": "Arjan van Bentem",
  "private": true,
  "homepage": "https://avbentem.github.io/lorawan-airtime-ui",
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run lint && react-scripts build",
    "test": "react-scripts test",
    "lint:es": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:style": "stylelint 'src/**/*'",
    "lint:pretty": "prettylint '**/*.{html,json,md,yaml}'",
    "lint": "npm run lint:es || npm run lint:style || npm run lint:pretty",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint",
    "src/**/*": "stylelint",
    "**/*.{html,json,md,yaml}": "prettylint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/dom": "^7.2.2",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/user-event": "^10.1.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-test-renderer": "^16.9.2",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.5",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^10.2.2",
    "node-sass": "^4.14.0",
    "prettier": "^2.0.5",
    "prettylint": "^1.0.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-github-corner": "^2.3.0",
    "react-markdown": "^4.3.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-test-renderer": "^16.13.1",
    "stylelint": "^13.3.3",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2",
    "typescript": "^3.8.3"
  }
}
